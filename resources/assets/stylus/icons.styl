	url('/fonts/guanacastevende.ttf?-sts7x8') format('truetype'),
	url('/fonts/guanacastevende.woff?-sts7x8') format('woff'),
	url('/fonts/guanacastevende.svg?-sts7x8#guanacastevende') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"],[class*=" glyphicon-"] {
